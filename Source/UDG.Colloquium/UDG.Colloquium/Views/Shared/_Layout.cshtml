@using System.Collections
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("styles", required: false)
    <meta name="description" content="The description of my page" />
</head>
<body>
   @RenderBody()
    <div class="footer">
        <div class="container">
            <div class="row">
                <img src="../../Content/images/bg_firm.gif" alt="UDG Gif" />
            </div>
            <div class="row">
                <div class="offsetLogo">
                    <a id="linkRectoria" href="http://www.vicerrectoria.udg.mx"><strong>©2010 - 2013 Centro Universitario de Ciencias Económico Administrativas</strong></a>
                </div>
            </div>
            <div class="row">
                <div class="offsetLogo">
                    COORDINACIÓN DE POSGRADOS CUCEA
                </div>
            </div>
            <div class="row">
                <div class="offsetLogo">
                    Periférico Norte N° 799, Núcleo Universitario Los Belenes, C.P. 45100, Zapopan, Jalisco, México.<br /><strong>Teléfono: +52 (33) 3770 3300.</strong>
                </div>
            </div>
        </div><br />
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/angularBreeze")
    @Scripts.Render("~/bundles/externalVendors")
    @Scripts.Render("~/bundles/bootstraping")
    @Scripts.Render("~/bundles/common")
    @Scripts.Render("~/bundles/app")
    @Scripts.Render("~/bundles/services")
    @Html.Partial("_ToastrPartial")
    @RenderSection("scripts", required: false)
</body>

</html>
