@using UDG.Colloquium.BL.ViewModels
@model PagedList.IPagedList<UserNamesViewModel>
@{
    ViewBag.Title = "ManageUsers";
}
<link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />
<h2>Manage Users</h2>

<form action="@Url.Action("ManageUsers")" method="get" role="search" class="form-inline" id="formUsers" data-colloquium-ajax="true" data-colloquium-target="#usersList">
    <div class="form-group">
        <input type="text" name="userName" class="form-control" placeholder="Search Username">
    </div>
    <div class="form-group">
        <input type="submit" class="btn btn-default"/>
    </div>
</form>

<p></p>
@Html.Partial("_UsersListPartial",Model)